// Doit afficher
// 52
// 666
// faux

variables
    entier x;
    entier a;
    booleen y;
fonctions
    entier f( entier x; )
    variables
        entier a;
    debut
        retourne x;
    fin
debut
    x = 42 + 5 * 2; // x = 52
    a = f(666); // a = 666
    y = (-a) == x; // faux

    ecrire x; // 52
    ecrire a; // 666
    ecrire y; // faux
fin